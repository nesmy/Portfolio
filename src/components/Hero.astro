  <!-- Photo grid -->
  <div class="w3-row" >
    <div  class="mySlides fade">
      <img src="/Portfolio/images/UrbanEngine.png" style="width:100%;height:650px" onclick="_gotoPage(2)" alt="project1">
    </div>
    <div  class="mySlides fade">
      <img src="/Portfolio/images/pingpong.png" style="width:100%;height:650px" onclick="_gotoPage(1)" alt="project2">
    </div>
    <div  class="mySlides fade">
      <img src="/Portfolio/images/GenesisOS.png" style="width:100%;height:650px" onclick="onClick1(this)" alt="project3">
