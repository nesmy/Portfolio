  <!-- Pagination -->
  <div class="w3-center w3-padding-32">
    <div class="w3-bar">
      <a class="w3-bar-item w3-button w3-hover-black" onclick="plusSlides(-1)">«</a>
      <a onclick="currentSlide(1)" class="dots w3-bar-item w3-button">1</span>
      <a onclick="currentSlide(2)" class="dots w3-bar-item w3-button">2</a>
      <a onclick="currentSlide(3)" class="dots w3-bar-item w3-button">3</a>
      <a class="w3-bar-item w3-button w3-hover-black" onclick="plusSlides(1)">»</a>
    </div>
    <div style="padding-top:5px">
        <button class="w3-bar-item w3-button w3-hover-black"><a class="w3-bar-item"style="text-decoration:none" href="https://nesmy.itch.io/" target="_blank" id="lang-en">More</a></button>
    </div>
  </div>

  <!-- Modal for full size images on click-->
  <div id="modal01" class="w3-modal w3-black" style="padding-top:0" onclick="this.style.display='none'">
    <span class="w3-button w3-black w3-xlarge w3-display-topright">×</span>
    <div class="w3-modal-content w3-animate-zoom w3-center w3-transparent w3-padding-64">
      <img id="img01" class="w3-image">
      <p id="caption"></p>
    </div>
  </div>

  <div id="modal02" class="w3-modal w3-black" style="padding-top:0">
    <span class="w3-button w3-black w3-xlarge w3-display-topright" onclick="this.parentElement.style.display='none'">×</span>
    <div class="w3-modal-content w3-animate-zoom w3-center w3-transparent w3-padding-64">
        <div id="screen_container1" >
          <div style="white-space: pre; font: 14px monospace; line-height: 14px"></div>
          <canvas style="display: none"></canvas>
      </div>
    </div>
  </div>
